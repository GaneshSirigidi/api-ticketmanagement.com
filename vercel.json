{
    "version": 2,
    "name": "ticketmanagement",
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/server.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/src/public/index.html"
        }
    ]
}